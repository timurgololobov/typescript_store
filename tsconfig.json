{
  "compilerOptions": {
    "outDir": "public/scripts",
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "allowJs": true,
    "declaration": true,
    "declarationDir": "public/scripts/types",
      "lib": [
        "es5", "es6", "dom", "dom.iterable"
    ],
      "noImplicitAny": true,
       "strictNullChecks": true,
       "strictFunctionTypes": true,
        "strictPropertyInitialization": true
  },
  "include": [
      "src/**/*"
      ],
  "files": [
    "src/index.ts"
  ]
}
